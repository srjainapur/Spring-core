<?xml version = "1.0" encoding = "UTF-8"?>

<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">

   <!-- Definition for javaCollection -->
   <bean id="javaCollection" class="com.spring.java.collection.JavaCollection">
   	<property name="addressList">
   		<list>
   			<value>INDIA</value>
   			<value>USA</value>
   			<value>INDIA</value>
   			<value>CANADA</value>
   		</list>
   	</property>
   	<property name="addressSet">
   		<set>
   			<value>INDIA</value>
            <value>Pakistan</value>
            <value>USA</value>
            <value>USA</value>
   		</set>
   	</property>
   	<property name="addressMap">
   		<map>
   			<entry key="1" value="INDIA"/>
   			<entry key="2" value="PAK"/>
   			<entry key="3" value="INDIA"/>
   			<entry key="4" value="CANADA"/>
   		</map>
   	</property>
   	<property name="addressProp">
   		<props>
   			<prop key="1">INDIA</prop>
   			<prop key="2">USA</prop>
   			<prop key="3">CANADA</prop>
   			<prop key="4">INIDA</prop>
   		</props>
   	</property>
   </bean>
</beans>